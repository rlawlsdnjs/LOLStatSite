{
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
  "functions": {
    "api/lolDetailMatch.ts": {
      "memory": 1024, 
      "maxDuration": 10 
    }
  },
  "routes": [
    {
      "src": "/api/*",
      "methods": ["GET", "POST"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}